#! c:/iverilog/bin/vvp
:ivl_version "0.10.0 (devel)" "(s20130827)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_00B6F270 .scope module, "testbench" "testbench" 2 1;
 .timescale 0 0;
v00B63768_0 .var "clk", 0 0;
v00B637C0_0 .var "key", 0 0;
v00B63818_0 .net "reset_sig", 0 0, v00B636B8_0;  1 drivers
S_00B6F340 .scope module, "RS_GEN" "powerup_reset" 2 9, 3 1 0, S_00B6F270;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "key"
    .port_info 1 /INPUT 1 "clk"
    .port_info 2 /OUTPUT 1 "rst"
P_00B6E678 .param/l "TICK" 0 3 2, +C4<01111>;
v00B6B2D8_0 .net "clk", 0 0, v00B63768_0;  1 drivers
v00B63660_0 .net "key", 0 0, v00B637C0_0;  1 drivers
v00B636B8_0 .var "rst", 0 0;
v00B63710_0 .var "tick_timer", 3 0;
E_00B6E6F0 .event posedge, v00B6B2D8_0;
    .scope S_00B6F340;
T_0 ;
    %ix/load 0, 1, 0;
    %assign/v0 v00B636B8_0, 0, 1;
    %ix/load 0, 4, 0;
    %assign/v0 v00B63710_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_00B6F340;
T_1 ;
    %wait E_00B6E6F0;
    %load/v 8, v00B63660_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/load 0, 4, 0;
    %assign/v0 v00B63710_0, 0, 1;
    %ix/load 0, 1, 0;
    %assign/v0 v00B636B8_0, 0, 1;
    %jmp T_1.1;
T_1.0 ;
    %load/v 8, v00B63710_0, 4;
    %cmpi/u 8, 0, 4;
    %jmp/0xz  T_1.2, 4;
    %ix/load 0, 1, 0;
    %assign/v0 v00B636B8_0, 0, 0;
    %jmp T_1.3;
T_1.2 ;
    %load/v 8, v00B63710_0, 4;
    %subi 8, 1, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v00B63710_0, 0, 8;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_00B6F270;
T_2 ;
    %vpi_call 2 13 "$dumpfile", "bench.vcd" {0 0};
    %vpi_call 2 14 "$dumpvars", 1'sb0, S_00B6F270 {0 0};
    %vpi_call 2 16 "$display", "starting testbench!!!!" {0 0};
    %movi 8, 300, 10;
T_2.0 %cmp/s 0, 8, 10;
    %jmp/0xz T_2.1, 5;
    %add 8, 1, 10;
    %delay 10, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00B63768_0, 0, 1;
    %delay 10, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00B63768_0, 0, 0;
    %jmp T_2.0;
T_2.1 ;
    %vpi_call 2 25 "$display", "finished OK!" {0 0};
    %vpi_call 2 26 "$finish" {0 0};
    %end;
    .thread T_2;
    .scope S_00B6F270;
T_3 ;
    %delay 1900, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00B637C0_0, 0, 1;
    %delay 90, 0;
    %ix/load 0, 1, 0;
    %assign/v0 v00B637C0_0, 0, 0;
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "testbench.v";
    ".//powerup_reset.v";
